{
  "manifest_version": 3,
  "name": "Actual Tcb Sync",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "92": "favicon.png"
  },
  "action": {
    "default_icon": "favicon.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": [
    "https://*.techcombank.com.vn/*"
  ]
}